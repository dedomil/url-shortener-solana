{
    "version": "0.1.0",
    "name": "url_shortener",
    "instructions": [
        {
            "name": "create",
            "accounts": [
                { "name": "urlAccount", "isMut": true, "isSigner": true },
                { "name": "signer", "isMut": true, "isSigner": true },
                { "name": "systemProgram", "isMut": false, "isSigner": false }
            ],
            "args": [{ "name": "url", "type": "string" }]
        }
    ],
    "accounts": [
        {
            "name": "UrlAccount",
            "type": {
                "kind": "struct",
                "fields": [
                    { "name": "authority", "type": "publicKey" },
                    { "name": "shortcode", "type": "string" },
                    { "name": "url", "type": "string" }
                ]
            }
        }
    ],
    "errors": [
        { "code": 6000, "name": "EmptyURL", "msg": "URL cannot be empty" }
    ]
}
